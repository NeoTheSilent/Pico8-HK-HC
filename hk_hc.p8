pico-8 cartridge // http://www.pico-8.com
version 41
__lua__

plx=16
ply=476
plmp=2
grav=2
jump=-12

function h_move()
 if (btn(0))--a second limit for room size will be decided later
	then 
	   plx-=plmp
	elseif (btn(1))--a second limit for room size will be decided later
	then
				plx+=plmp
	end
end

function gravity()
 --learn what's beneath player
 local grnd=mget(flr(plx/8),flr(ply/8)+1)
 
 if (grav!=2)
 then 
    grav+=2
 end  
 
 if ((fget(grnd)!=2)or(grav<0))
 then 
    ply+=grav
 end
end

function v_move()
 if ((btn(2))and(grav==2))
	then 
    grav=jump
 end
end

function _update()
 h_move()
 v_move()
 gravity() 
 camera(plx-64,ply-64)
  
end

function _draw()
 cls()
 map(0,0)
 spr(001,plx,ply)
 print(plx,plx+32,ply+32)
 print(ply,plx+42,ply+32)
 print(grav,plx+42,ply+42)
 print(mget(flr(plx/8),flr(ply/8)+1),plx+22,ply+22)
 print(fget(mget(flr(plx/8),flr(ply/8)+1)),plx+12,ply+22)
end

